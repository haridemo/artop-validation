<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.emf.compare.match.engine">
      <matchengine
            engineClass="org.artop.ecl.examples.sphinx.compare.match.SphinxMatchEngine"
            label="Sphinx Match Engine"
            namespace="http://www.artop.org/sphinx">
      </matchengine>
   </extension>

   <extension
         point="org.eclipse.emf.compare.diff.mergerprovider">
      <mergerprovider
            fileExtension="xml,sphinx"
            mergerProviderClass="org.artop.ecl.emf.compare.mergers.provider.TransactionalMergeProvider"
            priority="normal">
      </mergerprovider>
   </extension>

