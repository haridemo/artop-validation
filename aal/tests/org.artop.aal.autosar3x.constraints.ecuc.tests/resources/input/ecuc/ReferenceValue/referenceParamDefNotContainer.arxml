<?xml version="1.0" encoding="UTF-8"?>
<AUTOSAR xmlns="http://autosar.org/3.1.1">
	<TOP-LEVEL-PACKAGES>
    <AR-PACKAGE>
		<SHORT-NAME>AUTOSAR</SHORT-NAME>
		<ELEMENTS>
			<MODULE-DEF>
				<SHORT-NAME>Os</SHORT-NAME>
				<CONTAINERS>
					<PARAM-CONF-CONTAINER-DEF>
						<SHORT-NAME>OsApplication</SHORT-NAME>
						<REFERENCES>
								<REFERENCE-PARAM-DEF>
									<SHORT-NAME>OsAppScheduleTableRef</SHORT-NAME>
									<DESTINATION-REF DEST="PARAM-CONF-CONTAINER-DEF">/AUTOSAR/Os/OsScheduleTable</DESTINATION-REF>
								</REFERENCE-PARAM-DEF>
						</REFERENCES>
					</PARAM-CONF-CONTAINER-DEF>
					<PARAM-CONF-CONTAINER-DEF>
						<SHORT-NAME>OsScheduleTable</SHORT-NAME>
					</PARAM-CONF-CONTAINER-DEF>
          		</CONTAINERS>
        	</MODULE-DEF>
      	</ELEMENTS>
    </AR-PACKAGE>

    <AR-PACKAGE>
      <SHORT-NAME>OS_Package</SHORT-NAME>
      <ELEMENTS>
        <MODULE-CONFIGURATION>
          <SHORT-NAME>OS_ModuleConfiguration</SHORT-NAME>
          <DEFINITION-REF DEST="MODULE-DEF">/AUTOSAR/Os</DEFINITION-REF>
          <IMPLEMENTATION-CONFIG-VARIANT>VARIANT-PRE-COMPILE</IMPLEMENTATION-CONFIG-VARIANT>
          <CONTAINERS>
            <CONTAINER>
				<SHORT-NAME>OS_OsApplication</SHORT-NAME>
				<DEFINITION-REF DEST="PARAM-CONF-CONTAINER-DEF">/AUTOSAR/Os/OsApplication</DEFINITION-REF>
				<REFERENCE-VALUES>
					<REFERENCE-VALUE>
						<DEFINITION-REF DEST="REFERENCE-PARAM-DEF">/AUTOSAR/Os/OsApplication/OsAppScheduleTableRef</DEFINITION-REF>
						<!-- DEST is not of type CONTAINER -->
						<VALUE-REF DEST="MODULE-CONFIGURATION">/OS_Package/OS_ModuleConfiguration</VALUE-REF>
					</REFERENCE-VALUE>
				</REFERENCE-VALUES>
            </CONTAINER>     
            <CONTAINER>
				<SHORT-NAME>OS_OsScheduleTable</SHORT-NAME>
				<DEFINITION-REF DEST="PARAM-CONF-CONTAINER-DEF">/AUTOSAR/Os/OsScheduleTable</DEFINITION-REF>
            </CONTAINER>     
          </CONTAINERS>
        </MODULE-CONFIGURATION>
      </ELEMENTS>
    </AR-PACKAGE>

  </TOP-LEVEL-PACKAGES>
</AUTOSAR>