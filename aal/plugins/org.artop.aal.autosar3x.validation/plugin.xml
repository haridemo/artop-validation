<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.artop.ecl.emf.validation.registration">
      <model
            id="autosar3x"
            filter="_3x"
            Name="AUTOSAR 3.x"
            NsURI="http://autosar.org/3.1.1/10"
            class="autosar3x.genericstructure.infrastructure.ARObject">
      </model>
   </extension>

   <extension point="org.eclipse.emf.validation.constraintBindings">
        <clientContext id="autosar3x.ARObject.context">
            <enablement>
                <instanceof value="autosar3x.genericstructure.infrastructure.ARObject"/>
            </enablement>
        </clientContext>
   </extension>
  
   <extension point="org.eclipse.emf.transaction.listeners">
      <listener class="org.artop.aal.autosar3x.validation.listeners.ProblemMarkerIntegrity3xListener"/>
      <listener class="org.artop.aal.autosar3x.validation.listeners.AutomaticValidation3xListener"/>
   </extension>
  
</plugin>
