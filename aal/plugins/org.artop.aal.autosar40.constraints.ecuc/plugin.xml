<plugin>
   <extension
         point="org.eclipse.emf.validation.constraintProviders">
      <category
            id="org.artop.aal.autosar40.constraints.ecuc"
            mandatory="false"
            name="AAL Constraints for Ecuc Descriptions">
         Constraints for validating ECUC descriptions.
      </category>
      <category
            id="org.artop.aal.autosar40.constraints.ecuc/StructuralIntegrity"
            mandatory="false"
            name="Structural Integrity">
         Constraints for validating the structural integrity of ECUC descriptions.
      </category>
      <category
            id="org.artop.aal.autosar40.constraints.ecuc/Basic"
            mandatory="false"
            name="Basic Completeness and Consistency">
         Constraints for validating basic completeness and consistency of ECUC descriptions.
      </category>
      <constraintProvider
            cache="true"
            mode="Batch">
         <package
               namespaceUri="http://autosar.org/schema/r4.0/autosar40/ecucd">
         </package>
         <package
               namespaceUri="http://autosar.org/schema/r4.0/autosar40/ecucpd">
         </package>
         <constraints
               categories="org.artop.aal.autosar40.constraints.ecuc/Basic">
            <constraint
                  class="org.artop.aal.autosar40.constraints.ecuc.EcucModuleConfigurationValuesBasicConstraint"
                  id="org.artop.aal.autosar40.constraints.ecuc.ModuleConfigurationBasicConstraint_40"
                  lang="JAVA"
                  mode="Batch"
                  name="ModuleConfiguration"
                  severity="ERROR"
                  statusCode="0010013">
               <message>
                  {0}
               </message>
               <description>
                  Validates 'definition' of a ModuleConfiguration.
               </description>
               <target
                     class="EcucModuleConfigurationValues">
               </target>
            </constraint>
            <constraint
                  class="org.artop.aal.gautosar.constraints.ecuc.GContainerBasicConstraint"
                  id="org.artop.aal.autosar40.constraints.ecuc.ContainerBasicConstraint_40"
                  lang="JAVA"
                  mode="Batch"
                  name="Container"
                  severity="ERROR"
                  statusCode="0010014">
               <message>
                  {0}
               </message>
               <description>
                  Validates 'definition' of a Container.
               </description>
               <target
                     class="EcucContainerValue">
               </target>
            </constraint>
            <constraint
                  class="org.artop.aal.gautosar.constraints.ecuc.GParamConfMultiplicityBasicConstraint"
                  id="org.artop.aal.autosar40.constraints.ecuc.ParamConfMultiplicityBasicConstraint_40"
                  lang="JAVA"
                  mode="Batch"
                  name="ParamConfMultiplicityBasicConstraint"
                  severity="ERROR"
                  statusCode="0010001">
               <message>
                  {0}
               </message>
               <description>
                  Validates 'lowerMultiplicity' and 'upperMultiplicity' of ParamConfMultiplicity:
               If 'lowerMultiplicity' is set, then it shall be a positive integer.
               If 'upperMultiplicity' is set, then it shall be a positive integer or "*".
               </description>
               <target
                     class="EcucDefinitionElement">
               </target>
            </constraint>
            <constraint
                  class="org.artop.aal.autosar40.constraints.ecuc.EcucNumericalParamValueBasicConstraint"
                  id="org.artop.aal.autosar40.constraints.ecuc.EcucNumericalParamValueBasicConstraint_40"
                  lang="JAVA"
                  mode="Batch"
                  name="EcucNumericalParamValue"
                  severity="ERROR"
                  statusCode="0010008">
               <message>
                  {0}
               </message>
               <description>
                  Validates &apos;value&apos; and &apos;definition&apos; of a EcucNumericalParamValue.
               </description>
               <target
                     class="EcucNumericalParamValue">
               </target>
            </constraint>
            <constraint
                  class="org.artop.aal.autosar40.constraints.ecuc.EcucTextualParamValueBasicConstraint"
                  id="org.artop.aal.autosar40.constraints.ecuc.EcucTextualParamValueBasicConstraint_40"
                  lang="JAVA"
                  mode="Batch"
                  name="EcucTextualParamValue"
                  severity="ERROR"
                  statusCode="0010004">
               <message>
                  {0}
               </message>
               <description>
                  Validates&apos;value&apos; and &apos;definition&apos; of a EcucTextualParamValue.
               </description>
               <target
                     class="EcucTextualParamValue">
               </target>
            </constraint>
            <constraint
                  class="org.artop.aal.gautosar.constraints.ecuc.GReferenceValueBasicConstraint"
                  id="org.artop.aal.autosar40.constraints.ecuc.ReferenceValueBasicConstraint_40"
                  lang="JAVA"
                  mode="Batch"
                  name="ReferenceValue"
                  severity="ERROR"
                  statusCode="0010011">
               <message>
                  {0}
               </message>
               <description>
                  Validates 'value' and 'definition' of a ReferenceValue.
               </description>
               <target
                     class="EcucReferenceValue">
               </target>
            </constraint>
            <constraint
                  class="org.artop.aal.autosar40.constraints.ecuc.EcucInstanceReferenceValueBasicConstraint"
                  id="org.artop.aal.autosar40.constraints.ecuc.InstanceReferenceValueBasicConstraint_40"
                  lang="JAVA"
                  mode="Batch"
                  name="InstanceReferenceValue"
                  severity="ERROR"
                  statusCode="0010012">
               <message>
                  {0}
               </message>
               <description>
                  Validates 'value' and 'definition' of a InstanceReferenceValue.
               </description>
               <target
                     class="EcucInstanceReferenceValue">
               </target>
            </constraint>
            <constraint
                  class="org.artop.aal.gautosar.constraints.ecuc.GReferenceDefBasicConstraint"
                  id="org.artop.aal.autosar40.constraints.ecuc.ReferenceParamDefBasicConstraint_40"
                  lang="JAVA"
                  mode="Batch"
                  name="ReferenceParamDefBasicConstraint"
                  severity="ERROR"
                  statusCode="0010002">
               <message>
                  {0}
               </message>
               <description>
                  Validates 'destination' of ReferenceParamDef.   
               A resolvable 'destination' is expected.
               </description>
               <target
                     class="EcucReferenceDef">
               </target>
            </constraint>
            <constraint
                  class="org.artop.aal.gautosar.constraints.ecuc.GChoiceReferenceDefBasicConstraint"
                  id="org.artop.aal.autosar40.constraints.ecuc.ChoiceReferenceParamDefBasicConstraint_40"
                  lang="JAVA"
                  mode="Batch"
                  name="ChoiceReferenceParamDefBasicConstraint"
                  severity="ERROR"
                  statusCode="0010003">
               <message>
                  {0}
               </message>
               <description>
                  Validates 'destinations' of ChoiceReferenceParamDef. 
               At least one 'destination' is expected. All 'destinations' shall be resolvable.
               </description>
               <target
                     class="EcucChoiceReferenceDef">
               </target>
            </constraint>
         </constraints>
         <constraints
               categories="org.artop.aal.autosar40.constraints.ecuc/Multiplicity">
            <constraint
                  class="org.artop.aal.autosar40.constraints.ecuc.ModuleConfigurationSubContainerMultiplicityConstraint"
                  id="org.artop.aal.autosar40.constraints.ecuc.ModuleConfigurationSubContainerMultiplicityConstraint_40"
                  lang="JAVA"
                  mode="Batch"
                  name="ModuleConfigurationSubContainerMultiplicityConstraint"
                  severity="ERROR"
                  statusCode="0020005">
               <message>
                  {0}
               </message>
               <description>
                  Validates the multiplicity of the containers of a ModuleConfiguration.
               </description>
               <target
                     class="EcucModuleConfigurationValues">
               </target>
            </constraint>
            <constraint
                  class="org.artop.aal.autosar40.constraints.ecuc.ContainerSubContainerMultiplicityConstraint"
                  id="org.artop.aal.autosar40.constraints.ecuc.ContainerSubContainerMultiplicityConstraint_40"
                  lang="JAVA"
                  mode="Batch"
                  name="ContainerSubContainerMultiplicityConstraint"
                  severity="ERROR"
                  statusCode="0020004">
               <message>
                  {0}
               </message>
               <description>
                  Validates the multiplicity of the subContainers of a Container.
               </description>
               <target
                     class="EcucContainerValue">
               </target>
            </constraint>
            <constraint
                  class="org.artop.aal.gautosar.constraints.ecuc.GContainerParameterValueMultiplicityConstraint"
                  id="org.artop.aal.autosar40.constraints.ecuc.ContainerParameterValueMultiplicityConstraint_40"
                  lang="JAVA"
                  mode="Batch"
                  name="ContainerParameterValueMultiplicityConstraint"
                  severity="ERROR"
                  statusCode="0020006">
               <message>
                  {0}
               </message>
               <description>
                  Validates the multiplicity of the parameterValues of a Container.
               </description>
               <target
                     class="EcucContainerValue">
               </target>
            </constraint>
            <constraint
                  class="org.artop.aal.gautosar.constraints.ecuc.GContainerReferenceValueMultiplicityConstraint"
                  id="org.artop.aal.autosar40.constraints.ecuc.ContainerReferenceValueMultiplicityConstraint_40"
                  lang="JAVA"
                  mode="Batch"
                  name="ContainerReferenceValueMultiplicityConstraint"
                  severity="ERROR"
                  statusCode="0020007">
               <message>
                  {0}
               </message>
               <description>
                  Validates the multiplicity of the referenceValues of a Container.
               </description>
               <target
                     class="EcucContainerValue">
               </target>
            </constraint>
         </constraints>
      </constraintProvider>
   </extension>
   
     <extension point="org.eclipse.emf.validation.constraintBindings">    
        <binding
            context="autosar40.ARObject.context"
            category="org.artop.aal.autosar40.constraints.ecuc"/>
   </extension>

</plugin>
