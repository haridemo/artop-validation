<plugin>
   <extension point="org.eclipse.emf.validation.constraintProviders">
      <category
            id="org.artop.aal.autosar3x.constraints.ecuc"
            mandatory="false"
            name="AAL Constraints for Ecuc Descriptions">
      Constraints for validating ECUC descriptions.
      </category>
      <category
            id="org.artop.aal.autosar3x.constraints.ecuc/StructuralIntegrity"
            mandatory="false"
            name="Structural Integrity">
      Constraints for validating the structural integrity of ECUC descriptions.
      </category>
      <category
            id="org.artop.aal.autosar3x.constraints.ecuc/Basic"
            mandatory="false"
            name="Basic Completeness and Consistency">
      Constraints for validating basic completeness and consistency of ECUC descriptions.
      </category>
      
      <constraintProvider
            cache="true"
            mode="Batch">
         <package namespaceUri="http://autosar.org/3.1.4/autosar3x/ecucd"/>
         <package namespaceUri="http://autosar.org/3.1.4/autosar3x/ecucpd"/>

         <constraints categories="org.artop.aal.autosar3x.constraints.ecuc/Basic">
            <constraint
                  class="org.artop.aal.autosar3x.constraints.ecuc.ModuleConfigurationBasicConstraint"
                  id="org.artop.aal.autosar3x.constraints.ecuc.ModuleConfigurationBasicConstraint_3x"
                  lang="JAVA"
                  mode="Batch"
                  name="ModuleConfiguration"
                  severity="ERROR"
                  statusCode="0010013">
               <message>
                  {0}
               </message>
               <description>
                  Validates 'definition' of a ModuleConfiguration.
               </description>
               <target
                     class="ModuleConfiguration">
               </target>
            </constraint>
            <constraint
                  class="org.artop.aal.gautosar.constraints.ecuc.GContainerBasicConstraint"
                  id="org.artop.aal.autosar3x.constraints.ecuc.ContainerBasicConstraint_3x"
                  lang="JAVA"
                  mode="Batch"
                  name="Container"
                  severity="ERROR"
                  statusCode="0010014">
               <message>
                  {0}
               </message>
               <description>
                  Validates 'definition' of a Container.
               </description>
               <target
                     class="Container">
               </target>
            </constraint>
             <constraint
                  class="org.artop.aal.gautosar.constraints.ecuc.GParamConfMultiplicityBasicConstraint"
                  id="org.artop.aal.autosar3x.constraints.ecuc.ParamConfMultiplicityBasicConstraint_3x"
                  lang="JAVA"
                  mode="Batch"
                  name="ParamConfMultiplicityBasicConstraint"
                  severity="ERROR"
                  statusCode="0010001">
               <message>
                  {0}
               </message>
               <description>
               Validates 'lowerMultiplicity' and 'upperMultiplicity' of ParamConfMultiplicity:
               If 'lowerMultiplicity' is set, then it shall be a positive integer.
               If 'upperMultiplicity' is set, then it shall be a positive integer or "*".           
               </description>
               <target
                     class="ParamConfMultiplicity">
               </target>
            </constraint>
             <constraint
                   class="org.artop.aal.autosar3x.constraints.ecuc.BooleanValueBasicConstraint"
                   id="org.artop.aal.autosar3x.constraints.ecuc.BooleanValueBasicConstraint_3x"
                   lang="JAVA"
                   mode="Batch"
                   name="BooleanValue"
                   severity="ERROR"
                   statusCode="0010008">
                <message>
                   {0}
                </message>
                <description>
                   Validates 'value' and 'definition' of a BooleanValue.
                </description>
                <target
                      class="BooleanValue">
                </target>
             </constraint>
             <constraint
                   class="org.artop.aal.autosar3x.constraints.ecuc.IntegerValueBasicConstraint"
                   id="org.artop.aal.autosar3x.constraints.ecuc.IntegerValueBasicConstraint_3x"
                   lang="JAVA"
                   mode="Batch"
                   name="IntegerValue"
                   severity="ERROR"
                   statusCode="0010007">
                <message>
                   {0}
                </message>
                <description>
                   Validates 'value' and 'definition' of a IntegerValue.
                </description>
                <target
                      class="IntegerValue">
                </target>
             </constraint>
             <constraint
                   class="org.artop.aal.autosar3x.constraints.ecuc.FloatValueBasicConstraint"
                   id="org.artop.aal.autosar3x.constraints.ecuc.FloatValueBasicConstraint_3x"
                   lang="JAVA"
                   mode="Batch"
                   name="FloatValue"
                   severity="ERROR"
                   statusCode="0010009">
                <message>
                   {0}
                </message>
                <description>
                   Validates 'value' and 'definition' of a FloatValue.
                </description>
                <target
                      class="FloatValue">
                </target>
             </constraint>
             <constraint
                   class="org.artop.aal.autosar3x.constraints.ecuc.StringValueBasicConstraint"
                   id="org.artop.aal.autosar3x.constraints.ecuc.StringValueBasicConstraint_3x"
                   lang="JAVA"
                   mode="Batch"
                   name="StringValue"
                   severity="ERROR"
                   statusCode="0010004">
                <message>
                   {0}
                </message>
                <description>
                   Validates 'value' and 'definition' of a StringValue.
                </description>
                <target
                      class="StringValue">
                </target>
             </constraint>
             <constraint
                   class="org.artop.aal.autosar3x.constraints.ecuc.FunctionNameValueBasicConstraint"
                   id="org.artop.aal.autosar3x.constraints.ecuc.FunctionNameValueBasicConstraint_3x"
                   lang="JAVA"
                   mode="Batch"
                   name="FunctionNameValue"
                   severity="ERROR"
                   statusCode="0010005">
                <message>
                   {0}
                </message>
                <description>
                   Validates 'value' and 'definition' of a FunctionNameValue.
                </description>
                <target
                      class="FunctionNameValue">
                </target>
             </constraint>
             <constraint
                   class="org.artop.aal.autosar3x.constraints.ecuc.LinkerSymbolValueBasicConstraint"
                   id="org.artop.aal.autosar3x.constraints.ecuc.LinkerSymbolValueBasicConstraint_3x"
                   lang="JAVA"
                   mode="Batch"
                   name="LinkerSymbolValue"
                   severity="ERROR"
                   statusCode="0010006">
                <message>
                   {0}
                </message>
                <description>
                   Validates 'value' and 'definition' of a LinkerSymbolValue.
                </description>
                <target
                      class="LinkerSymbolValue">
                </target>
             </constraint>
             <constraint
                   class="org.artop.aal.autosar3x.constraints.ecuc.EnumerationValueBasicConstraint"
                   id="org.artop.aal.autosar3x.constraints.ecuc.EnumerationValueBasicConstraint_3x"
                   lang="JAVA"
                   mode="Batch"
                   name="EnumerationValue"
                   severity="ERROR"
                   statusCode="0010010">
                <message>
                   {0}
                </message>
                <description>
                   Validates 'value' and 'definition' of a FloatValue.
                </description>
                <target
                      class="EnumerationValue">
                </target>
             </constraint>
             <constraint
                   class="org.artop.aal.gautosar.constraints.ecuc.GReferenceValueBasicConstraint"
                   id="org.artop.aal.autosar3x.constraints.ecuc.ReferenceValueBasicConstraint_3x"
                   lang="JAVA"
                   mode="Batch"
                   name="ReferenceValue"
                   severity="ERROR"
                   statusCode="0010011">
                <message>
                   {0}
                </message>
                <description>
                   Validates 'value' and 'definition' of a ReferenceValue.
                </description>
                <target
                      class="ReferenceValue">
                </target>
             </constraint>
             <constraint
                   class="org.artop.aal.autosar3x.constraints.ecuc.InstanceReferenceValueBasicConstraint"
                   id="org.artop.aal.autosar3x.constraints.ecuc.InstanceReferenceValueBasicConstraint_3x"
                   lang="JAVA"
                   mode="Batch"
                   name="InstanceReferenceValue"
                   severity="ERROR"
                   statusCode="0010012">
                <message>
                   {0}
                </message>
                <description>
                   Validates 'value' and 'definition' of a InstanceReferenceValue.
                </description>
                <target
                      class="InstanceReferenceValue">
                </target>
             </constraint>
            
              <constraint
                  class="org.artop.aal.gautosar.constraints.ecuc.GReferenceDefBasicConstraint"
                  id="org.artop.aal.autosar3x.constraints.ecuc.ReferenceParamDefBasicConstraint_3x"
                  lang="JAVA"
                  mode="Batch"
                  name="ReferenceParamDefBasicConstraint"
                  severity="ERROR"
                  statusCode="0010002">
               <message>
                  {0}
               </message>
               <description>
               Validates 'destination' of ReferenceParamDef.   
               A resolvable 'destination' is expected.       
               </description>
               <target
                     class="ReferenceParamDef">
               </target>
            </constraint>
            
            <constraint
                  class="org.artop.aal.gautosar.constraints.ecuc.GChoiceReferenceDefBasicConstraint"
                  id="org.artop.aal.autosar3x.constraints.ecuc.ChoiceReferenceParamDefBasicConstraint_3x"
                  lang="JAVA"
                  mode="Batch"
                  name="ChoiceReferenceParamDefBasicConstraint"
                  severity="ERROR"
                  statusCode="0010003">
               <message>
                  {0}
               </message>
               <description>
               Validates 'destinations' of ChoiceReferenceParamDef. 
               At least one 'destination' is expected. All 'destinations' shall be resolvable.     
               </description>
               <target
                     class="ChoiceReferenceParamDef">
               </target>
            </constraint>
            
         
            
            
            
            
      </constraints>
      <constraints categories="org.artop.aal.autosar3x.constraints.ecuc/StructuralIntegrity">
         <constraint
               class="org.artop.aal.autosar3x.constraints.ecuc.ModuleConfigurationSubContainerMultiplicityConstraint"
               id="org.artop.aal.autosar3x.constraints.ecuc.ModuleConfigurationSubContainerMultiplicityConstraint_3x"
               lang="JAVA"
               mode="Batch"
               name="ModuleConfigurationSubContainerMultiplicityConstraint"
               severity="ERROR"
               statusCode="0020005">
            <message>
               {0}
            </message>
            <description>
               Validates the multiplicity of the containers of a ModuleConfiguration.
            </description>
            <target
                  class="ModuleConfiguration">
            </target>
         </constraint>
         <constraint
               class="org.artop.aal.gautosar.constraints.ecuc.GContainerStructuralIntegrityConstraint"
               id="org.artop.aal.autosar3x.constraints.ecuc.ContainerStructuralIntegrityConstraint_3x"
               lang="JAVA"
               mode="Batch"
               name="ContainerStructuralIntegrityConstraint"
               severity="ERROR"
               statusCode="0020003">
            <message>
               {0}
            </message>
            <description>
               Validates the structural integrity of a Container i.e. Does the ECUC Parameter Definition allows the container in the parent Container or parent MouleConfiguration?
            </description>
            <target
                  class="Container">
            </target>
         </constraint>
         <constraint
               class="org.artop.aal.autosar3x.constraints.ecuc.ContainerSubContainerMultiplicityConstraint"
               id="org.artop.aal.autosar3x.constraints.ecuc.ContainerSubContainerMultiplicityConstraint_3x"
               lang="JAVA"
               mode="Batch"
               name="ContainerSubContainerMultiplicityConstraint"
               severity="ERROR"
               statusCode="0020004">
            <message>
               {0}
            </message>
            <description>
               Validates the multiplicity of the subContainers of a Container.
            </description>
            <target
                  class="Container">
            </target>
         </constraint>
         <constraint
               class="org.artop.aal.gautosar.constraints.ecuc.GContainerParameterValueMultiplicityConstraint"
               id="org.artop.aal.autosar3x.constraints.ecuc.ContainerParameterValueMultiplicityConstraint_3x"
               lang="JAVA"
               mode="Batch"
               name="ContainerParameterValueMultiplicityConstraint"
               severity="ERROR"
               statusCode="0020006">
            <message>
               {0}
            </message>
            <description>
               Validates the multiplicity of the parameterValues of a Container.
            </description>
            <target
                  class="Container">
            </target>
         </constraint>
         <constraint
               class="org.artop.aal.gautosar.constraints.ecuc.GContainerReferenceValueMultiplicityConstraint"
               id="org.artop.aal.autosar3x.constraints.ecuc.ContainerReferenceValueMultiplicityConstraint_3x"
               lang="JAVA"
               mode="Batch"
               name="ContainerReferenceValueMultiplicityConstraint"
               severity="ERROR"
               statusCode="0020007">
            <message>
               {0}
            </message>
            <description>
               Validates the multiplicity of the referenceValues of a Container.
            </description>
            <target
                  class="Container">
            </target>
         </constraint>
         <constraint
               class="org.artop.aal.gautosar.constraints.ecuc.GParameterValueStructuralIntegrityConstraint"
               id="org.artop.aal.autosar3x.constraints.ecuc.ParameterValueStructuralIntegrityConstraint_3x"
               lang="JAVA"
               mode="Batch"
               name="ParameterValueStructuralIntegrityConstraint"
               severity="ERROR"
               statusCode="0020002">
            <message>
               {0}
            </message>
            <description>
               Validates the structural integrity of a ParameterValue. i.e. Does the ECUC Parameter Definition allows the parameter in the parent Container?
            </description>
            <target
                  class="ParameterValue">
            </target>
         </constraint>      
            <constraint
                  class="org.artop.aal.gautosar.constraints.ecuc.GConfigReferenceValueStructuralIntegrityConstraint"
                  id="org.artop.aal.autosar3x.constraints.ecuc.ConfigReferenceValueStructuralIntegrityConstraint_3x"
                  lang="JAVA"
                  mode="Batch"
                  name="ConfigReferenceValueStructuralIntegrityConstraint"
                  severity="ERROR"
                  statusCode="0020001">
               <message>
                  {0}
               </message>
               <description>
               Validates the structural integrity of a ConfigReferenceValue. i.e. Does the ECUC Parameter Definition allows a the reference in the parent Container?  
               </description>
               <target
                     class="ConfigReferenceValue">
               </target>
            </constraint>  
            
            
            
            
         </constraints>     
      </constraintProvider>
   </extension>
   <extension point="org.eclipse.emf.validation.constraintBindings">    
        <binding
            context="autosar3x.ARObject.context"
            category="org.artop.aal.autosar3x.constraints.ecuc"/>
   </extension>

</plugin>
