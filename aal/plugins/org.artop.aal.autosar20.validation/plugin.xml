<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.artop.ecl.emf.validation.registration">
      <model
            id="autosar20"
            filter="_20"
            Name="AUTOSAR 2.0"
            NsURI="http://autosar.org/2.0.0/autosar20/gs/i"
            class="autosar20.genericstructure.infrastructure.ARObject">
      </model>
   </extension>
 
    <extension point="org.eclipse.emf.validation.constraintBindings">
        <clientContext id="autosar20.ARObject.context">
            <enablement>
                <instanceof value="autosar20.genericstructure.infrastructure.ARObject"/>
            </enablement>
        </clientContext>
   </extension>

   <extension
         point="org.artop.ecl.emf.editingDomainFactoryListeners">
      <listener
            id="org.artop.aal.autosar20.validation.internal.ValidationEditingDomainFactoryListener"
            class="org.artop.aal.autosar20.validation.internal.ValidationEditingDomainFactoryListener">
         <applicableFor
               metaModelDescriptorIdPattern="org.artop.aal.autosar20" />
      </listener>
   </extension>

</plugin>
